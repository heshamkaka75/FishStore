@model FishStore.Models.CenterFishAgency

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>CenterFishAgency</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="CFA_id" />
            <div class="form-group">
                <label asp-for="Weight" class="control-label"></label>
                <input asp-for="Weight" class="form-control" />
                <span asp-validation-for="Weight" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FishNo" class="control-label"></label>
                <select asp-for="FishNo" class="form-control" asp-items="ViewBag.FishNo"></select>
                <span asp-validation-for="FishNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BoatNo" class="control-label"></label>
                <select asp-for="BoatNo" class="form-control" asp-items="ViewBag.BoatNo"></select>
                <span asp-validation-for="BoatNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DeliveryDate" class="control-label"></label>
                <input asp-for="DeliveryDate" class="form-control" />
                <span asp-validation-for="DeliveryDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
